{
  "compilerOptions": {
    "module": "CommonJS",
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "outDir": "lib",
    "rootDir": "src",
    "sourceMap": true,
    "strict": true,
    //"isolatedModules": true,
    "target": "es2017"
  },
  "compileOnSave": true,
"include": ["src", "scripts"]
}
