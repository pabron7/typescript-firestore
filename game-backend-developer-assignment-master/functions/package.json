    "seed": "ts-node src/scripts/seed.ts",
    "buildImage": "docker build -t softgames-emulator .."
  },
  "dependencies": {
    "firebase-admin": "^12.6.0",
    "firebase-functions": "^6.0.1",
    "morgan": "^1.10.0"
